<body class='contrast-red '>
    <header>
        <div class='navbar'>
            <div class='navbar-inner'>
                <div class='container-fluid'>
                    <a class='brand' href='index-2.html'>
                        <img src="~/Content/assets/images/login/logoTLeanNegativoMini.png" style="width: 12%;"/>
                        <span class='hidden-phone'>TLean Consulting</span>
                    </a>
                    <a class='toggle-nav btn pull-left' href='#'>
                        <i class='icon-reorder'></i>
                    </a>
                    <ul class='nav pull-right'>
                        <li class='dropdown dark user-menu'>
                            <a class='dropdown-toggle' data-toggle='dropdown' href='#'>
                                <img alt='Mila Kunis' height='23' src='@Url.Content("~/Content/assets/images/avatar.jpg")' width='23' />
                                <span class='user-name hidden-phone'>Administrador</span>
                                <b class='caret'></b>
                            </a>
                            <ul class='dropdown-menu'>
                                <li>
                                    <a href='@Url.Action("LogOut","Login",new { area = String.Empty})'>
                                        <i class='icon-signout'></i>
                                        Cerrar Sesión
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </header>
    <div id='wrapper'>
        <div id='main-nav-bg'></div>
        @{Html.RenderPartial("_Sidebar");}
        <section id='content'>
            <div class='container-fluid'>
                <div class='row-fluid' id='content-wrapper'>
                    <div class='span12'>
                        <div class="row-fluid">
                            <div class="span12">
                                <div class="page-header">
                                    <h1 class="pull-left">
                                        <i class="icon-@ViewBag.PageIcon"></i>
                                        <span>@ViewBag.Title</span>
                                    </h1>
                                    @if (IsSectionDefined("Breadcrumbs"))
                                    {
                                        <div class="pull-right">
                                            <ul class="breadcrumb">
                                                <li class="breadcrumb-item"><a href="@Url.Action("Dashboard", "Login", new { Area = String.Empty})"><i class="icon icon-home" style="padding-right: 0px;"></i> Home</a></li>
                                                <li class="separator"><i class="icon-angle-right"></i></li>
                                                @RenderSection("Breadcrumbs", false)
                                                <li class="active">@ViewBag.Title</li>
                                            </ul>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                        @MessageHelpers.ShowMessages(TempData)
                        @RenderBody()                        
                    </div>
                </div>                
            </div>
        </section>
