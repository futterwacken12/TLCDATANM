    <style>
        .bg {
            background-image: url('@Url.Content("~/Content/assets/images/login/fondo.png")') !important;
            background-position: center center;
            background-repeat: no-repeat;
            background-attachment: fixed;
            background-size: cover;
        }
    </style>
