<body class='contrast-red sign-in bg'>
    <div id='wrapper'>
        <div class='application'>
            <div class='application-content'>
                <a href="#">
                    <img src="@Url.Content("~/Content/assets/images/login/TLeanHorizontalNegativo.png")"/>
                </a>
            </div>
        </div>
        <div class='controls'>
            <div class='caret'></div>
            <div class='form-wrapper'>
                <h1 class='text-center'>Iniciar Sesión</h1>
                <form accept-charset="UTF-8" method="post">
                    <div style="margin:0;padding:0;display:inline">
                        <input name="utf8" type="hidden" value="&#x2713;" />
                    </div>
                    <div class='row-fluid'>
                        <div class='span12 icon-over-input'>
                            @Html.TextBoxFor( x => x.UserName, new { @class = "span12", @placeholder = "Código de usuario" })
                            <i class='icon-user muted'></i>
                        </div>
                    </div>
                    <div class='row-fluid'>
                        <div class='span12 icon-over-input'>
                            @Html.TextBoxFor(x => x.Password, new { @class = "span12", @placeholder = "Contraseña" , @type = "password" })
                            <i class='icon-lock muted'></i>
                        </div>
                    </div>

                    <button class="btn btn-block" name="button" type="submit">Ingresar</button>
                </form>
                <div class='text-center'>
                    <hr class='hr-normal' />
                    <a href="#">¿Olvidó su contraseña?</a>
                </div>
            </div>
        </div>
    </div>
